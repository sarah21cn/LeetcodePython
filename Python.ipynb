    "# 闭包\n",
    "\n",
    "from functools import reduce\n",
    "\n",
    "def lazy_sum(array):\n",
    "    def add():\n",
    "        return reduce(lambda x, y: x + y, array)\n",
    "    return add\n",
    "\n",
    "a = lazy_sum([1,2,3])\n",
    "print(a())"
   ]
